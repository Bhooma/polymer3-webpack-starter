    "@vaadin/vaadin-grid": "^5.4.8",
    "@vaadin/vaadin-item": "^2.1.1",
    "@vaadin/vaadin-list-box": "^1.1.1",
    "@vaadin/vaadin-lumo-styles": "^1.5.0",
    "@vaadin/vaadin-notification": "^1.4.0",
